<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="20251029000001" author="jhipster">
        <loadData
            file="config/liquibase/fake-data/sport.csv"
            separator="_"
            tableName="sport">
            <column name="id" type="numeric"/>
            <column name="name" type="string"/>
        </loadData>
    </changeSet>

    <changeSet id="20251029000002" author="jhipster">
        <loadData
            file="config/liquibase/fake-data/court.csv"
            separator=","
            tableName="court">
            <column name="id" type="numeric"/>
            <column name="name" type="string"/>
            <column name="sport_id" type="numeric"/>
        </loadData>
    </changeSet>

</databaseChangeLog>
